{
  "name": "api-events",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src -d dist",
    "dev": "nodemon build/server",
    "start": "node ./build/server",
    "postinstall": "postinstall-build dist",
    "docker:build": "yarn build && docker build -t garciadiazjaime/api-events .",
    "docker:run": "docker run -d -p 49330:3030 --name events garciadiazjaime/api-events",
    "docker:push": "docker push garciadiazjaime/api-events",
    "docker:pull": "docker pull garciadiazjaime/api-events"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "convict": "^4.0.2",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.12.3",
    "mongoose": "^5.0.0-rc1",
    "morgan": "^1.9.0",
    "postinstall-build": "^5.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "nodemon": "^1.14.7"
  }
}
